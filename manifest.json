{
  "manifest_version": 2,
  "name": "Genshin CodeEnter",
  "version": "1.0.1",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "*://*.hoyoverse.com/",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "__MSG_extensionDescription__",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
